                game:GetService("ReplicatedStorage"):WaitForChild("PerformKillEffect"):FireServer(v.Name)
            end
        end
    end)
end

-- Variables to track active connections
local activeConnections = {}

-- Function to swing the sword at the predicted respawn time
function PredictRespawn(targetPlayer)
    local respawnTime = RespawnTime or 5  -- Adjust the respawn time (seconds)
    local predictTime = 0.25  -- Time before respawn to swing the sword (0.25 seconds)
    
    -- Loop to handle continuous prediction and swing
    while getgenv().Predict do
        task.wait(0.1)  -- Delay to prevent excessive CPU usage

        if targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
            local humanoid = targetPlayer.Character.Humanoid
            if humanoid.Health <= 0 then
                print("Target", targetPlayer.Name, "has died. Predicting respawn...")

                -- Predict the time before respawn (0.25 seconds before respawn)
                task.delay(respawnTime - predictTime, function()
                    if not getgenv().Predict then
                        print("Prediction toggled off during respawn prediction.")
                        return
                    end
                    
                    if humanoid.Health == 0 then  -- If the player is still dead, swing the sword
                        print("Swinging sword at predicted respawn time for:", targetPlayer.Name)
                        local localPlayer = game.Players.LocalPlayer
                        local sword = localPlayer.Backpack:FindFirstChild("Sword") or localPlayer.Character:FindFirstChild("Sword")

                        if sword and sword:FindFirstChild("Handle") then
                            -- Equip sword and prepare attack
                            sword.Parent = localPlayer.Character
                            sword.Handle.Massless = true
                            sword.Handle.Size = Vector3.new(15, 15, 15)
                            spawn(function()
                                sword:Activate()
                                task.wait(0.05)
                                sword:Activate()
                                task.wait(0.05)
                                sword:Activate()
                            end)

                            -- Attack at the predicted respawn location
                            if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                local targetCFrame = localPlayer.Character.Sword.Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                targetPlayer.Character.HumanoidRootPart.CFrame = targetCFrame
                                sword:Activate()
                            else
                                print("Predicted respawn location not found for:", targetPlayer.Name)
                            end
                        else
                            print("Sword not found for local player.")
                        end
                    end
                end)
                
                -- Wait until player respawns
                while targetPlayer.Character == nil or targetPlayer.Character:FindFirstChild("Humanoid") == nil or targetPlayer.Character.Humanoid.Health <= 0 do
                    task.wait(0.1)
                end

                -- Player has respawned, teleport them to the local player
                if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    print("Teleporting target back to local player:", targetPlayer.Name)
                    targetPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)  -- Adjust offset if needed
                end
            end
        end
    end
end

-- Function to start the prediction process for all target players
function StartPrediction()
    for _, player in pairs(game.Players:GetPlayers()) do
        if table.find(Targets, player.Name) and not activeConnections[player.Name] then
            print("Starting prediction for:", player.Name)
            activeConnections[player.Name] = spawn(function()
                PredictRespawn(player)
            end)
        end
    end

    -- Monitor new players joining
    game.Players.PlayerAdded:Connect(function(player)
        if table.find(Targets, player.Name) and not activeConnections[player.Name] then
            print("Starting prediction for new player:", player.Name)
            activeConnections[player.Name] = spawn(function()
                PredictRespawn(player)
            end)
        end
    end)
end

-- Function to stop the prediction process and clear all active connections
function StopPrediction()
    -- Disconnect all active connections
    for playerName, thread in pairs(activeConnections) do
        activeConnections[playerName] = nil
    end
    print("Prediction stopped.")
end

-- Function to toggle prediction on or off
function TogglePrediction(state)
    if state then
        print("Prediction enabled")
        StartPrediction()
    else
        print("Prediction disabled")
        StopPrediction()
    end
end

-- Monitor the toggle state
getgenv().Predict = false -- Default state
spawn(function()
    local lastState = getgenv().Predict
    while true do
        task.wait(0.1) -- Monitor for state changes
        if lastState ~= getgenv().Predict then
            TogglePrediction(getgenv().Predict)
            lastState = getgenv().Predict
        end
    end
end)



function LoopKill()
    spawn(function()
        if getgenv().RenderSteppedMethod == true then
            local renderStep
            renderStep = game:GetService("RunService").RenderStepped:Connect(function()
                if getgenv().LoopKill == true then
                    for i, v in pairs(game.Players:GetPlayers()) do
                        if table.find(Targets, v.Name) then
                            if game.Players.LocalPlayer.Character then
                                if v.Character then
                                    if v.Character:FindFirstChild("HumanoidRootPart") then
                                        if v.Character:FindFirstChild("Humanoid") then
                                            if v.Character.Humanoid.Health ~= 0 then
                                                if game.Players.LocalPlayer.Character then
                                                    if getgenv().Predict == true then

                                                    else
                                                        if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword") then
                                                            if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                                game.Players.LocalPlayer.Backpack:FindFirstChild("Sword").Parent = game.Players.LocalPlayer.Character
                                                            end
                                                        end
                                                        if game.Players.LocalPlayer.Character:FindFirstChild("Sword") then
                                                            if game.Players.LocalPlayer.Character:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                                v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                                game.Players.LocalPlayer.Character.Sword:Activate()
                                                                game.Players.LocalPlayer.Character.Sword.Handle.Massless = true
                                                                game.Players.LocalPlayer.Character.Sword.Handle.Size = Vector3.new(15, 15, 15)
                                                                v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                                game.Players.LocalPlayer.Character.Sword:Activate()
                                                                v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                            end
                                                        end
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                else
                    renderStep:Disconnect()
                end
            end)
        else
            while getgenv().LoopKill == true do
                task.wait()
                for i, v in pairs(game.Players:GetPlayers()) do
                    if table.find(Targets, v.Name) then
                        if game.Players.LocalPlayer.Character then
                            if v.Character then
                                if v.Character:FindFirstChild("HumanoidRootPart") then
                                    if v.Character:FindFirstChild("Humanoid") then
                                        if v.Character.Humanoid.Health ~= 0 then
                                            if game.Players.LocalPlayer.Character then
                                                if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword") then
                                                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                        game.Players.LocalPlayer.Backpack:FindFirstChild("Sword").Parent = game.Players.LocalPlayer.Character
                                                    end
                                                end
                                                if game.Players.LocalPlayer.Character:FindFirstChild("Sword") then
                                                    if game.Players.LocalPlayer.Character:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                        v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                        game.Players.LocalPlayer.Character.Sword:Activate()
                                                        game.Players.LocalPlayer.Character.Sword.Handle.Massless = true
                                                        game.Players.LocalPlayer.Character.Sword.Handle.Size = Vector3.new(15, 15, 15)
                                                        v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                        game.Players.LocalPlayer.Character.Sword:Activate()
                                                        v.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Sword").Handle.CFrame + Vector3.new(2.5, 2.5, 2.5)
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
end

function TeleportLoopKill()
    if getgenv().RenderSteppedMethod == true then
        spawn(function()
            local renderStep
            renderStep = game:GetService("RunService").RenderStepped:Connect(function()
                if getgenv().TeleportLoopKill == true then
                    for i, v in pairs(game.Players:GetPlayers()) do
                        if table.find(Targets, v.Name) then
                            if game.Players.LocalPlayer.Character then
                                if v.Character then
                                    if v.Character:FindFirstChild("HumanoidRootPart") then
                                        if v.Character:FindFirstChild("Humanoid") then
                                            if v.Character.Humanoid.Health ~= 0 then
                                                if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword") then
                                                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                        game.Players.LocalPlayer.Backpack:FindFirstChild("Sword").Parent = game.Players.LocalPlayer.Character
                                                    end
                                                end
                                                if game.Players.LocalPlayer.Character:FindFirstChild("Sword") then
                                                    if game.Players.LocalPlayer.Character:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                        game.Players.LocalPlayer.Character.Sword:Activate()
                                                        game.Players.LocalPlayer.Character.Sword.Handle.Massless = true
                                                        game.Players.LocalPlayer.Character.Sword.Handle.Size = Vector3.new(15, 15, 15)
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                        game.Players.LocalPlayer.Character.Sword:Activate()
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                else
                    renderStep:Disconnect()
                end
            end)
        end)
    else
        spawn(function()
            while getgenv().TeleportLoopKill == true do
                task.wait()
                for i, v in pairs(game.Players:GetPlayers()) do
                    if table.find(Targets, v.Name) then
                        if game.Players.LocalPlayer.Character then
                            if v.Character then
                                if v.Character:FindFirstChild("HumanoidRootPart") then
                                    if v.Character:FindFirstChild("Humanoid") then
                                        if v.Character.Humanoid.Health ~= 0 then
                                            if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword") then
                                                if game.Players.LocalPlayer.Backpack:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                    game.Players.LocalPlayer.Backpack:FindFirstChild("Sword").Parent = game.Players.LocalPlayer.Character
                                                end
                                            end
                                            if game.Players.LocalPlayer.Character:FindFirstChild("Sword") then
                                                if game.Players.LocalPlayer.Character:FindFirstChild("Sword"):FindFirstChild("Handle") then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                    game.Players.LocalPlayer.Character.Sword:Activate()
                                                    game.Players.LocalPlayer.Character.Sword.Handle.Massless = true
                                                    game.Players.LocalPlayer.Character.Sword.Handle.Size = Vector3.new(15, 15, 15)
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                    game.Players.LocalPlayer.Character.Sword:Activate()
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character:FindFirstChild("HumanoidRootPart").CFrame
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end

function SpinBot()
    spawn(function()
        local renderStep
        renderStep = game:GetService("RunService").RenderStepped:Connect(function()
            if getgenv().SpinBot == true then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, getgenv().SpinBotSpeed, 0)
            else
                renderStep:Disconnect()
            end
        end)
    end)
end
